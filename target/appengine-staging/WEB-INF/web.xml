<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
		 version="3.0">

	<!-- JSPC servlet mappings start -->

    <servlet>
        <servlet-name>org.apache.jsp.WEB_002dINF.text_jsp</servlet-name>
        <servlet-class>org.apache.jsp.WEB_002dINF.text_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.add_005fcandidate_jsp</servlet-name>
        <servlet-class>org.apache.jsp.add_005fcandidate_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.result_jsp</servlet-name>
        <servlet-class>org.apache.jsp.result_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.index_jsp</servlet-name>
        <servlet-class>org.apache.jsp.index_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.email_005fimport_jsp</servlet-name>
        <servlet-class>org.apache.jsp.email_005fimport_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.backendservlet_jsp</servlet-name>
        <servlet-class>org.apache.jsp.backendservlet_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.text_jsp</servlet-name>
        <servlet-class>org.apache.jsp.text_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.voting_005fportal_jsp</servlet-name>
        <servlet-class>org.apache.jsp.voting_005fportal_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.datepicker_jsp</servlet-name>
        <servlet-class>org.apache.jsp.datepicker_jsp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.WEB_002dINF.text_jsp</servlet-name>
        <url-pattern>/WEB-INF/text.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.add_005fcandidate_jsp</servlet-name>
        <url-pattern>/add_candidate.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.result_jsp</servlet-name>
        <url-pattern>/result.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.index_jsp</servlet-name>
        <url-pattern>/index.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.email_005fimport_jsp</servlet-name>
        <url-pattern>/email_import.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.backendservlet_jsp</servlet-name>
        <url-pattern>/backendservlet.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.text_jsp</servlet-name>
        <url-pattern>/text.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.voting_005fportal_jsp</servlet-name>
        <url-pattern>/voting_portal.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.datepicker_jsp</servlet-name>
        <url-pattern>/datepicker.jsp</url-pattern>
    </servlet-mapping>

<!-- JSPC servlet mappings end -->

<servlet-mapping>
		<servlet-name>Backend</servlet-name>
		<url-pattern>/backend</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Backend</servlet-name>
		<servlet-class>com.example.appengine.java8.BackendServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DatePicker</servlet-name>
		<url-pattern>/backend/datepicker</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DatePicker</servlet-name>
		<servlet-class>com.example.appengine.java8.DatePickerServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>add_candidate</servlet-name>
		<url-pattern>/backend/add_candidate</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>add_candidate</servlet-name>
		<servlet-class>com.example.appengine.java8.AddCandidateServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>email_import</servlet-name>
		<url-pattern>/backend/email_import</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>email_import</servlet-name>
		<servlet-class>com.example.appengine.java8.EmailImportServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>voting_portal</servlet-name>
		<url-pattern>/user/voting_portal</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>voting_portal</servlet-name>
		<servlet-class>com.example.appengine.java8.VotingPortalServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>result_servlet</servlet-name>
		<url-pattern>/result</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>result_servlet</servlet-name>
		<servlet-class>com.example.appengine.java8.ResultServlet</servlet-class>
	</servlet>


	<!---Un comment when you use with Google Cloude-->

	<servlet-mapping>
		<servlet-name>date_pick_form</servlet-name>
		<url-pattern>/backend/date_pick_form</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>date_pick_form</servlet-name>
		<servlet-class>com.example.appengine.java8.DatePickerServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>add_candidate_post</servlet-name>
		<url-pattern>/backend/adding_candidate_database</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>add_candidate_post</servlet-name>
		<servlet-class>com.example.appengine.java8.AddCandidateServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>email_import_data</servlet-name>
		<url-pattern>/backend/email_import_data</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>email_import_data</servlet-name>
		<servlet-class>com.example.appengine.java8.EmailImportServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>adding_vote_database</servlet-name>
		<url-pattern>/user/adding_vote_database</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>adding_vote_database</servlet-name>
		<servlet-class>com.example.appengine.java8.VotingPortalServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>send_mail</servlet-name>
		<url-pattern>/backend/send_mail</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>send_mail</servlet-name>
		<servlet-class>com.example.appengine.java8.BackendServlet</servlet-class>
	</servlet>



<!--	==============================================================================	-->

<!--	This code use for authentication of google -->

	<security-constraint>
		<web-resource-collection>
			<web-resource-name>admin</web-resource-name>
			<url-pattern>/backend/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>admin</role-name>
		</auth-constraint>
	</security-constraint>

	<security-constraint>
		<web-resource-collection>
			<web-resource-name>user</web-resource-name>
			<url-pattern>/user/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>*</role-name>
		</auth-constraint>
	</security-constraint>

<!-- ==================================================================================  -->

</web-app>