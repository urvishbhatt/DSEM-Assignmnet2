<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
		 version="3.0">

	<servlet-mapping>
		<servlet-name>Backend</servlet-name>
		<url-pattern>/backend</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Backend</servlet-name>
		<servlet-class>com.example.appengine.java8.BackendServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DatePicker</servlet-name>
		<url-pattern>/datepicker</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DatePicker</servlet-name>
		<servlet-class>com.example.appengine.java8.DatePickerServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>add_candidate</servlet-name>
		<url-pattern>/add_candidate</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>add_candidate</servlet-name>
		<servlet-class>com.example.appengine.java8.AddCandidateServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>email_import</servlet-name>
		<url-pattern>/email_import</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>email_import</servlet-name>
		<servlet-class>com.example.appengine.java8.EmailImportServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>voting_portal</servlet-name>
		<url-pattern>/voting_portal</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>voting_portal</servlet-name>
		<servlet-class>com.example.appengine.java8.VotingPortalServlet</servlet-class>
	</servlet>


	<!---Un comment when you use with Google Cloude-->

	<servlet-mapping>
		<servlet-name>add_candidate_post</servlet-name>
		<url-pattern>/adding_candidate_database</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>add_candidate_post</servlet-name>
		<servlet-class>com.example.appengine.java8.AddCandidateServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>email_import_data</servlet-name>
		<url-pattern>/email_import_data</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>email_import_data</servlet-name>
		<servlet-class>com.example.appengine.java8.EmailImportServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>send_mail</servlet-name>
		<url-pattern>/send_mail</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>send_mail</servlet-name>
		<servlet-class>com.example.appengine.java8.BackendServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>date_pick_form</servlet-name>
		<url-pattern>/date_pick_form</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>date_pick_form</servlet-name>
		<servlet-class>com.example.appengine.java8.DatePickerServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>adding_vote_database</servlet-name>
		<url-pattern>/adding_vote_database</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>adding_vote_database</servlet-name>
		<servlet-class>com.example.appengine.java8.VotingPortalServlet</servlet-class>
	</servlet>

